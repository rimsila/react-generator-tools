{
    "name": "react-generator-tools",
    "version": "1.7.3",
    "description": "",
    "authors": {
        "name": "Sila"
    },
    "main": "lib/index.js",
    "scripts": {
        "setup": "yarn install; cd example; yarn install; cd ..",
        "start": "cross-env UMI_UI=1 && UMI_ENV=dev APP_ROOT=example umi dev",
        "dev": "concurrently --kill-others \"yarn watch\" \"yarn start\"",
        "watch": "father-build --watch",
        "build": "rm -rf dist lib && cross-env NODE_ENV=prod father-build",
        "ui:build": "ui-build",
        "prettier": "prettier --parser=typescript --write '{src,ui}/**/*.ts?(x)'",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage",
        "test:update": "umi-test --updateSnapshot"
    },
    "lint-staged": {
        "*.ts?(x)": [
            "git add"
        ]
    },
    "devDependencies": {
        "@ant-design/pro-card": "^1.11.12",
        "@ant-design/pro-descriptions": "^1.7.7",
        "@ant-design/pro-layout": "^6.17.1",
        "@ant-design/pro-list": "^1.7.2",
        "@ant-design/pro-skeleton": "^1.0.1",
        "@ant-design/pro-table": "^2.35.1",
        "@testing-library/react": "^9.4.0",
        "@testing-library/react-hooks": "^3.2.1",
        "@types/classnames": "^2.3.1",
        "@types/codemirror": "^5.60.2",
        "@types/faker": "^5.5.7",
        "@types/jest": "^25.1.3",
        "@types/lodash": "^4.14.171",
        "@types/node": "^13.7.7",
        "@umijs/fabric": "^2.6.2",
        "@umijs/preset-ui": "^2.2.9",
        "@umijs/test": "^3.0.10",
        "@umijs/test-utils": "^1.0.0",
        "@umijs/ui-builder": "^0.0.6",
        "@umijs/ui-types": "2.2.0",
        "antd": "4.16.8",
        "body-parser": "^1.18.2",
        "classnames": "^2.3.1",
        "codemirror": "^5.62.2",
        "concurrently": "^6.2.0",
        "copy-to-clipboard": "^3.3.1",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "faker": "^4.1.0",
        "father-build": "^1.19.6",
        "immer": "^9.0.5",
        "lerna": "^4.0.0",
        "lint-staged": "^10.0.8",
        "lodash": "^4.17.21",
        "npm-run-all": "^4.1.5",
        "pify": "^5.0.0",
        "puppeteer": "^1.20.0",
        "query-string": "^6.11.1",
        "react": "^17.0.2",
        "react-codemirror2": "^7.2.0",
        "react-dom": "^17.0.2",
        "react-test-renderer": "^16.9.0",
        "test-umi-plugin": "^0.1.0",
        "typescript": "^4.3.5",
        "umi": "^3.5.13",
        "use-immer": "^0.6.0",
        "yorkie": "^2.0.0"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "files": [
        "dist",
        "lib"
    ],
    "dependencies": {}
}
